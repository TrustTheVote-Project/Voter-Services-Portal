- @page_title = 'Event log'

.row
  .span16
    %table#log_records.zebra-striped
      %thead
        %tr
          %th         Voter ID
          %th.subject Doc Type
          %th.action  Action
          %th         Notes
          %th.date    Date
      %tbody
        = render @log_records

.row
  .span16
    = paginate @log_records
